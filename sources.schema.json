{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "type": "array",
    "items": {
        "$ref": "#/definitions/Source"
    },
    "definitions": {
        "Source": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "name": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "author": {
                    "type": "string"
                },
                "authorUrl": {
                    "type": "string",
                    "qt-uri-protocols": [
                        "https"
                    ]
                },
                "sourceUrl": {
                    "type": "string",
                    "format": "uri",
                    "qt-uri-protocols": [
                        "https"
                    ],
                    "qt-uri-extensions": [
                        ".js",
                        ".json"
                    ]
                },
                "repositoryUrl": {
                    "type": "string",
                    "format": "uri",
                    "qt-uri-protocols": [
                        "https"
                    ],
                    "qt-uri-extensions": [
                        ".ccc-plugin"
                    ]
                },
                "scriptUrl": {
                    "type": "string",
                    "qt-uri-protocols": [
                        "https"
                    ],
                    "qt-uri-extensions": [
                        ".js"
                    ]
                },
                "version": {
                    "type": "integer"
                },
                "iconUrl": {
                    "type": "string",
                    "qt-uri-protocols": [
                        "https"
                    ],
                    "qt-uri-extensions": [
                        ".jpg",
                        ".png"
                    ]
                },
                "id": {
                    "type": "string",
                    "format": "uuid"
                },
                "packages": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Package"
                    }
                },
                "allowEval": {
                    "type": "boolean"
                },
                "allowUrls": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "_tags": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "scriptSignature": {
                    "type": "string"
                },
                "scriptPublicKey": {
                    "type": "string"
                },
                "_feeds": {
                    "$ref": "#/definitions/Feeds"
                },
                "platformUrl": {
                    "type": "string",
                    "qt-uri-protocols": [
                        "https"
                    ]
                },
                "authentication": {
                    "$ref": "#/definitions/Authentication"
                },
                "settings": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Setting"
                    }
                },
                "supportedClaimTypes": {
                    "type": "array",
                    "items": {
                        "type": "integer"
                    }
                },
                "constants": {
                    "$ref": "#/definitions/Constants"
                },
                "_customButtons": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/CustomButton"
                    }
                },
                "iconUrl_": {
                    "type": "string",
                    "format": "uri",
                    "qt-uri-protocols": [
                        "https"
                    ],
                    "qt-uri-extensions": [
                        ".jpg"
                    ]
                },
                "sourceUrl_": {
                    "type": "string",
                    "format": "uri",
                    "qt-uri-protocols": [
                        "https"
                    ],
                    "qt-uri-extensions": [
                        ".json"
                    ]
                },
                "allowAllHttpHeaderAccess": {
                    "type": "boolean"
                },
                "websiteUrl": {
                    "type": "string",
                    "format": "uri",
                    "qt-uri-protocols": [
                        "https"
                    ]
                },
                "subscriptionRateLimit": {
                    "type": "integer"
                },
                "captcha": {
                    "$ref": "#/definitions/CAPTCHA"
                },
                "primaryClaimFieldType": {
                    "type": "integer"
                }
            },
            "required": [],
            "title": "Source"
        },
        "CustomButton": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "text": {
                    "type": "string"
                },
                "url": {
                    "type": "string",
                    "format": "uri",
                    "qt-uri-protocols": [
                        "https"
                    ]
                },
                "classes": {
                    "type": "string"
                }
            },
            "required": [],
            "title": "CustomButton"
        },
        "Feeds": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "commits": {
                    "type": "string",
                    "format": "uri",
                    "qt-uri-protocols": [
                        "https"
                    ],
                    "qt-uri-extensions": [
                        ".atom"
                    ]
                },
                "releases": {
                    "type": "string",
                    "format": "uri",
                    "qt-uri-protocols": [
                        "https"
                    ],
                    "qt-uri-extensions": [
                        ".atom"
                    ]
                }
            },
            "required": [],
            "title": "Feeds"
        },
        "Authentication": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "loginUrl": {
                    "type": "string",
